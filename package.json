{
  "name": "slidereditor",
  "displayName": "sliderEditor",
  "description": "testing custom webUI extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "slidereditor.openCustomEditor",
        "title": "Open with Slider Editor"
      }
    ],
    "customEditors": [
      {
        "viewType": "mySliderEditor",
        "displayName": "Piano Roll Editor",
        "selector": [
          {
            "filenamePattern": "*.pianoroll.json"
          }
        ]
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == .pianoroll.json",
          "command": "slidereditor.openCustomEditor",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
